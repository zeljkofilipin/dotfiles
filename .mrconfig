# default

[DEFAULT]
lib = 
	 update_git_repository () {
	   git checkout master &&
	   git branch | grep -vE 'master' | xargs git branch -D &&
	   git pull origin master
	 }



# update homebrew and tools installed via homebrew

[/Users/z]
update = 
	 brew update &&
	 brew upgrade &&
	 gem update --system &&
	 gem install bundler &&
	 npm install npm@latest -g &&
	 diff .mrconfig Documents/github/zeljkofilipin/dotfiles/.mrconfig



